    "@urban-mapping/common-utils": "^0.0.108",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.5.5",
    "bluebird": "^3.7.2",
    "cesium": "^1.69.0",
    "convex-hull": "^1.0.3",
    "coordinate-parser": "^1.0.3",
    "fast-exif": "^1.0.1",
    "geolib": "^3.2.1",
    "geolocation-utils": "^1.2.2",
    "js-quantities": "^1.7.5",
